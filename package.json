{
	"name": "events-free-spa",
	"version": "0.1.0",
	"private": true,
	"proxy": "http://localhost:3090",
	"engines": {
		"node": "8.9.1"
	},
	"scripts": {
		"build-css": "node-sass-chokidar --include-path ./client/src --include-path ./node_modules client/src/ -o client/src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./client/src --include-path ./node_modules client/src/ -o client/src/ --watch --recursive",
		"start-client": "node scripts/start.js",
		"start": "npm-run-all -p watch-css start-client",
		"build": "npm run build-css && node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"test-server": "mocha server/test",
		"start-server": "nodemon --watch ./server --exec npm run babel-node",
		"babel-node": "set NODE_ENV=test&& babel-node -- ./server/app.js",
		"babel-node2": "babel -d ./server/app.js --presets node6 --out-file ./server/app2.js",
		"postinstall": "npm run build",
		"ss": "npm run start-server"
	},
	"dependencies": {
		"async": "^2.5.0",
		"axios": "^0.16.2",
		"body-parser": "^1.17.2",
		"chalk": "1.1.3",
		"cheerio": "^1.0.0-rc.2",
		"chrono-node": "^1.3.5",
		"compression": "^1.7.0",
		"dotenv": "4.0.0",
		"dotenv-expand": "4.0.1",
		"express": "^4.15.3",
		"file-loader": "1.1.5",
		"fs-extra": "3.0.1",
		"lodash": "^4.17.4",
		"moment": "^2.18.1",
		"moment-timezone": "^0.5.13",
		"mongoose": "^4.11.5",
		"morgan": "^1.8.2",
		"newrelic": "^2.2.0",
		"node-cron": "^1.2.0",
		"node-horseman": "^3.3.0",
		"nodemailer": "^4.0.1",
		"nodemon": "^1.12.1",
		"object-assign": "4.1.1",
		"phantomjs-prebuilt": "^2.1.15",
		"promise": "8.0.1",
		"raf": "3.4.0",
		"react": "^16.2.0",
		"react-dev-utils": "^5.0.0",
		"react-dom": "^16.2.0",
		"react-error-overlay": "^1.0.10",
		"react-redux": "^5.0.1",
		"react-router-config": "1.0.0-beta.4",
		"react-router-dom": "4.2.2",
		"react-share": "^1.15.0",
		"redux": "^3.6.0",
		"redux-logger": "^2.7.4",
		"redux-thunk": "^2.1.0",
		"socket.io": "^2.0.3",
		"socket.io-client": "^2.0.3",
		"ssl-root-cas": "^1.2.4",
		"tress": "^1.0.3",
		"url-loader": "0.6.2",
		"vk-io": "^3.1.7",
		"whatwg-fetch": "2.0.3"
	},
	"devDependencies": {
		"autoprefixer": "7.1.6",
		"babel-cli": "^6.24.1",
		"babel-core": "6.26.0",
		"babel-eslint": "7.2.3",
		"babel-jest": "20.0.3",
		"babel-loader": "7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-node6": "^11.0.0",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chai": "^4.1.2",
		"css-loader": "0.28.7",
		"eslint": "4.10.0",
		"eslint-config-airbnb": "^15.0.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "2.39.1",
		"eslint-plugin-import": "2.8.0",
		"eslint-plugin-jsx-a11y": "5.1.1",
		"eslint-plugin-react": "7.4.0",
		"extract-text-webpack-plugin": "3.0.2",
		"html-webpack-plugin": "2.29.0",
		"jest": "20.0.4",
		"mocha": "^3.5.0",
		"node-sass": "^4.0.0",
		"node-sass-chokidar": "0.0.3",
		"npm-run-all": "^4.1.1",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.8",
		"sass-loader": "^4.1.0",
		"style-loader": "0.19.0",
		"sw-precache-webpack-plugin": "0.11.4",
		"webpack": "3.8.1",
		"webpack-dev-server": "2.9.4",
		"webpack-manifest-plugin": "1.3.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,mjs}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"mjs",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"config": {
		"ghooks": {
			"pre-push": "npm run test-server"
		}
	}
}
